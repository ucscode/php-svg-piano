{
    "name": "ucscode/uss-element",
    "description": "UssElement is a PHP library for building and manipulating HTML elements programmatically.",
    "keywords": ["php", "html", "dom", "element", "library"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Uchenna Ajah",
            "email": "uche23mail@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "autoload": {
        "psr-4": {
            "Ucscode\\UssElement\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ucscode\\UssElement\\Test\\": "tests/"
        }
    },
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10",
        "phpstan/phpstan": "^2.1"
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    }
}
